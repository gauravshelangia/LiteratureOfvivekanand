<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vivekanand.literature.literatureofvivekanand">

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/om_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/om_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutUs" />
        <activity android:name=".Gallery" />
        <activity android:name=".Settings" />
        <activity android:name=".EnglishActivity" />
        <activity android:name=".BengaliActivity" />
        <activity android:name=".BookWebView"></activity>
    </application>

</manifest>